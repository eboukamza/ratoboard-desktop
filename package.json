{
  "name": "ratoboard-desktop",
  "version": "1.0.0-beta",
  "author": "Elias Zain <ezain2t@gmail.com>",
  "private": true,
  "main": "www/electron.js",
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "electron": "electron .",
    "test": "karma start ./test-config/karma.conf.js",
    "test-coverage": "npm run test -- --single-run --coverage",
    "gh-pages": "ngh --dir www --message=\"new release\"",
    "package-linux": "electron-packager . --prune --overwrite --platform=linux --arch=x64 --icon=assets/incon.png  --out=dist",
    "package-win": "electron-packager . --asar --prune --overwrite --platform=win32 --icon=assets/icon/favicon.ico --arch=ia32 --out=dist",
    "dist": "build",
    "release": "ionic-app-scripts build --prod && build --config config/build-electron-release.json"
  },
  "config": {
    "ionic_copy": "./config/custom.copy.js"
  },
  "repository": {
      "type": "git",
      "url": "git+https://github.com/eboukamza/ratoboard-desktop.git"
    },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@ionic/storage": "2.1.3",
    "iohook": "0.1.13",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "node-mouse": "^0.0.2",
    "robotjs": "~0.5.1",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.0",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.8",
    "angular-cli-ghpages": "^0.5.1",
    "angular2-template-loader": "^0.6.2",
    "codecov": "^1.0.1",
    "codelyzer": "~4.0.1",
    "electron": "~1.8.4",
    "electron-packager": "^11.1.0",
    "electron-builder": "~20.5.1",
    "html-loader": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine": "^2.5.3",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "null-loader": "^0.1.1",
    "ts-loader": "~2.0.3",
    "ts-node": "~3.0.2",
    "typescript": "2.4.2"
  },
  "iohook": {
    "targets": [
      "electron-57"
    ],
    "platforms": [
      "linux",
      "win32"
    ],
    "arches": [
      "x64"
    ]
  },
  "description": "ratoboard destktop"
}
